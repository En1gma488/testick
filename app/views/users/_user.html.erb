<td><%= link_to user.email, user %> </td>
 
<td>
	<%= form_for(user) do |f| %>
		<%= f.select(:roll, User.rolls.keys.map {|role| [role.titleize,role]}) %>
		<td>
			<button><%= f.submit 'Change Role' %></button>
		</td>
	<% end %>
</td>
<td><%= link_to 'Delete', user_path(user), method: :delete, data: {confirm: "Are you sure?"} %></td>



